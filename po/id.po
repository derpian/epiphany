#, c-format
msgid "Press %s to exit fullscreen"
msgstr "Tekan %s untuk keluar dari layar penuh"

#: ../embed/ephy-embed.c:542
msgid "ESC"
msgstr "ESC"

#: ../embed/ephy-embed.c:542
msgid "F11"
msgstr "F11"

#: ../embed/ephy-embed-utils.c:62
#, c-format
msgid "Send an email message to “%s”"
msgstr "Kirim pesan surel ke \"%s\""

